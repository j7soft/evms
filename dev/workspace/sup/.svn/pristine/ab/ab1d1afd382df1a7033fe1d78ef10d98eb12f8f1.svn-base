}
.box_materials_01 .box_intro_01 p{
  font-size: 0.875rem;
  word-break: keep-all;
}
.box_materials_01 .box_intro_01>div{
  position: absolute; bottom: 0; left: 0;
  width: 100%; overflow: hidden;
}
.box_materials_01 .box_intro_03{
	position:relative;
  margin-top: 80px;
  box-sizing: border-box;
}
.box_materials_01 .box_intro_03 .img_box{
	width:100%;
	overflow:hidden;
}
.box_materials_01 .box_intro_03 .img_box img{
	width:100%;
	position:relative;
	top:-72px;
}
.box_materials_01 .box_intro_03 p{
  margin-top: 70px;
  padding-bottom:50px;
  border-bottom:1px solid #ddd;
}
.box_materials_01 .box_intro_03 h5{
	position: absolute;
    bottom: 170px;
    background: #fff;
	display: inline-block;
    padding: 35px;
	font-size:1.875rem;
}
.box_materials_01>.box_right{
  width: 47%; 
}
.box_materials_01 .box_intro_02_box{
	position:relative;
}
.box_materials_01 .box_intro_02{
  width:60%;
  margin-top: 108px; padding: 35px 200px 35px 35px;
  border:1px solid #ddd;
  box-sizing: border-box;
}

.box_materials_01 .box_intro_02_box .img_box{
	position: absolute;
    top: 50px;
    right: 0;
	width:50%;
}
.box_materials_01 .box_intro_02_box .img_box img{
	width:100%;
}
.box_materials_01 .box_intro_02 p{
  margin-top: 30px;
}
.box_materials_02{
  margin-top: 70px;
}

.box_materials_02 h4{
      margin: 30px 0 30px;
    text-align: left;
    font-size: 1.875rem;
    font-weight:500;
}
.box_materials_02>div{
  width: 100%;
}
.box_materials_02 .img_box{
	display:none;
	width:60%;
}
.box_materials_02 .img_box img{
	width:100%;
}
.box_materials_02 h5{
	font-size: 1.375rem;
	margin: 30px 0 10px;
}
.box_materials_02 .online_service{

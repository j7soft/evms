}
.ml10 li:first-of-type{
  margin-left: 0;
}
.box_materials_02 .p_major_business{
  font-size: 0.75rem; 
}
.box_materials_02 .p_major_business p span{
  position: relative;
  top: -3px;
}
.vision_logic_hr{margin:40px 0;}
.vision_logic_hr ul li{float:left; width:32%;height:350px;text-align:center;border:1px solid #63b746;margin-right:1%;}
.vision_logic_hr ul li:last-child{margin-right:0;}
.vision_logic_hr ul li h5{padding:15px 10px;margin:0;background-color: #63b746;color:#fff;font-weight:bold;margin-bottom:28px;}
.vision_logic_hr ul li h5 em{display:block;font-size:14px;font-weight:400;}
.vision_logic_hr ul li p{padding:0 35px;text-align:left;margin-bottom:15px;}
.vision_logic_hr ul li p span{display: inline-block; width:5px;height:5px;background-color: #63b746;float: left;margin-top:12px;margin-bottom: 15px;margin-right: 10px;
border-radius: 50px;}
.bar_hr{display: block;width: 40px;height: 2px;background-color: #63b746;margin-bottom:15px;}



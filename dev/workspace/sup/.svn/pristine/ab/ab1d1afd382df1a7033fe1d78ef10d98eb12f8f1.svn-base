/* 소재자료실 페이지 */
.materials_reference{
  width: 1200px;
  margin: 0 auto;
}
.materials .community_table thead{
  border-top: 1px solid #d0d2d3
}
.text_meterials{
  width:90%; margin:0 auto;
  max-width:1200px;
  margin-bottom: 20px;color: #707070
}
.text_meterials span{
  position: relative;top: -3px
}

/* 소재이야기 페이지 */
/*.materials_story_area{
  margin-top: 200px;
}*/
/* #materials_wrap .materials_story .content_h3bx{
	width: 90%;
} */
.materials_story_area p{
  line-height: 27px; word-break: keep-all; text-align: justify;
}
.box_left{
  float: left
}
.box_right{
  float: right;
}
.box_materials_story{
  max-width: 1200px;
  width:90%;
  margin: 0 auto 90px;
}
.box_materials_story>div{
  width: 31%;
}
.box_materials_story>div.materials_earth{
  width: 38%;
  padding: 0 40px;
  box-sizing: border-box;
}
.materials_earth img{
  width: 100%;
}
.box_materials_story .title_materials_story{
  margin-top: 15px; margin-bottom: 25px;
  font-size: 1rem;color: #63b746;line-height: 27px; font-weight:300;
  text-transform: uppercase;
}
.box_materials_story .title_materials_story p:nth-child(1){
	font-size: 1.875rem;
    margin-bottom: 10px;
}
.box_materials_story p:last-of-type{
  margin-top: 0; 
}
.line_bottom{
  position: relative;
}
.line_bottom:after{
  content: "";
  display: block;
  position: absolute; top: 22px;
  width: 99%; height: 1px;
  background: #666
}
.list_materials_story li:nth-of-type(2n-1){
  background: #f5f5f5;
}
.list_materials_story li>div{
  max-width: 1200px;
  width:90%;
  margin: 0 auto;
}
.list_materials_story li>div>div{
  width: 48%;
}
.list_materials_story li .box_left{
  padding: 70px 0 50px  
}
.list_materials_story li .box_right{
  width: 43%;
  margin-left: 7%; padding: 60px 0 80px;
  text-align: center
}
.list_materials_story h4{
  margin-bottom: 30px;
  font-size: 1.75rem;
  font-weight:300;
}
.list_materials_story h4 span{
	display:block;
	margin-left:30px;
}
.text_materials_story{
  border-top: 1px solid #e1e1e1;
}
.text_materials_story .title_materials_story, .text_materials_story .p_materials_story{
  max-width: 1200px;
  width:90%;
  margin: 0 auto;
}
.text_materials_story .title_materials_story{
  margin-top: 40px; margin-bottom: 20px;
  font-size: 1.5rem; font-weight:300;
  color:#63b746;
}
.text_materials_story .sub_title_materials_story{
	font-size:1.25rem;
	color:#333;
	font-weight:400;
}
.text_materials_story .title_materials_story p:nth-child(1){
	margin-bottom:10px;
}
.text_materials_story .p_materials_story{
  margin-bottom: 30px;
}
.text_center p{text-align:center}

.text_materials_story .img_box{
	max-width: 1200px;width:90%; margin: 0 auto;
	border-top:1px solid #e1e1e1;
}
.text_materials_story .img_box img{
	display:none;
}
.list_major_business{
  width: 1200px;
  margin: 50px auto;
  padding-top: 50px;
  border-top: 1px solid #e1e1e1;
}
.list_major_business li{
  float: left;
  width: 49%; height: 95px;
  margin-right: 2%;
  border: 3px solid #ccc;
  box-sizing: border-box;
}
.list_major_business li:nth-of-type(4n){
  margin-right: 0;
}
.list_major_business li>div{
  float: left;
  height: 100%;
}
.list_major_business li .box_major_img{
  width: 46.1%;
  background: #ebebeb;
}
.list_major_business li .box_major_arrow{
  width: 7.8%;
  background: #fff;
}
.list_major_business li .box_major_arrow img{
  display: block;
  position: relative; top: 50%;
  margin: 0 auto;
  transform: translateY(-60%);
  -webkit-transform: translateY(-60%);
  -ms-transform: translateY(-60%);
  -moz-transform: translateY(-60%);
  -o-transform: translateY(-60%);
}

/* 사업개요 페이지 */
.materials_intro{
  text-align: justify;
}
.materials_intro_area{width:90%;max-width:1200px; margin: 0 auto;}
.materials_intro_area h5{
  font-size: 1.125rem;
    font-weight: 400;
}
.materials_intro_area p{
  line-height: 27px;
}
.materials_intro_area h4{
  font-size: 1.875rem; font-weight: 500; color: #000;
  text-align:center;
}
.materials_intro_area>p{
  margin: 15px 0 40px;
  text-align: center;
  font-size:0.875rem;
}
.box_materials_01>.box_left{
  width: 53%;
}
.box_materials_01 .box_intro_01{
  text-align:center;
  padding-top: 50px;
}

.simple_schedule{
	width: 380px;
	padding: 10px;
	box-sizing: border-box;
	background-color: #fff;
	position: absolute;
	top: 0; left: 103%;
	overflow: hidden;
	box-shadow:0 5px 15px #ccc;	
	z-index: 100;
}
.simple_schedule:after{
	content:'';
	display:block;
	clear:both;
}
.simple_schedule .img_box{
	width: 110px; height: 85px;
	background-color: #ddd;
	float: left;
	margin-right: 10px;
}
.simple_schedule .img_box img{
	width: 100%;
}
.simple_schedule .content_box{
	width: 240px;
	float: left;
}
.simple_schedule .content_box .btn_wrap{
	margin-bottom: 10px;
}
.simple_schedule .content_box .btn_wrap:after{
	content:'';
	display:block;
	clear:both;
}
.simple_schedule .content_box .btn_wrap p{
	float:left;
	width: 70px; height: 25px;
	line-height: 25px !important;
	border: 1px solid #2d2d2d;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	text-align: center;
	margin-right: 5px;
	color:#2d2d2d;
}
.simple_schedule .content_box .btn_wrap p.on{
	background-color: #ccc;
	border-color: #ccc;
	color:#fff;
}
.simple_schedule .content_box .text_box{
	
}
.simple_schedule .content_box .text_box dl{
	margin-bottom: 0;
}
.simple_schedule .content_box .text_box dt{
	width: 100%;
	font-size: 11px;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	color:#2d2d2d;
	font-weight: bold;
}
.simple_schedule .content_box .text_box dd{
	margin-bottom: 0;
	font-size: 11px;
	text-align: left;	
	color:#2d2d2d;
}
#calendar *{
	overflow: visible;
}
.fc-scroller{
	overflow: visible !important;
}

.fc-center{}
.fc-center h2{
	font-size: 20px;
}
.fc-center h2 span{
	font-size: 64px;
	font-weight: bold;
	color:#2d2d2d;
	height: 70px;
	line-height: 70px;
}
.fc-toolbar{
	position: relative;
}
.fc-center{
	position: absolute;
	left: 50%;
	
	-webkit-transform: translate(-50%, 0);
	-moz-transform: translate(-50%, 0);
	-o-transform: translate(-50%, 0);
	transform: translate(-50%, 0);
}
.fc-left{
	position: relative;
	left: 50%;
	top: -6px;
	
	-webkit-transform: translate(-50%, 0);
	-moz-transform: translate(-50%, 0);
	-o-transform: translate(-50%, 0);
	transform: translate(-50%, 0);
}
.fc-left .fc-button-group{
	
}
.fc-left .fc-today-button{
	display:none !important;
}
.fc-right .fc-button-group{
	display: none !important;
}
.fc-corner-left{
	position: relative;
	left: -60px;
}
.fc-corner-right{
	position: relative;
	right: -60px;
}
.fc-corner-left,
.fc-corner-right{
	background-color: transparent !important;
	background-image: none !important;
	border: 0 !important;
	box-shadow: none !important;
	font-size: 20px !important;
}
.fc-unthemed tr:first-of-type{
	border: 0;
}
.fc-unthemed th{
	border: 0 !important;
}
.fc-head-container{
	height: 35px;
	border: 0 !important;
	text-indent: 20px;
}
.fc-day-header{
	text-align: left !important;
}
.fc-nonbusiness,
.fc-unthemed td.fc-today{
	background-color: transparent !important;
}
.fc-day-number{
	float: none !important;
	/*pointer-events: none;*/
}
.fc-day-top{
	padding: 5px !important;
}
.mobile_header{
	display: none;
}



.mobile_header h1{
	overflow: hidden;
	text-indent: -9999px;
	width: 149px; height: 34px;
	background: url(../img/common/mobile_logo.svg) center no-repeat;
	position: absolute;
	left: 50%; top: 50%;
	margin-top: -17px; margin-left: -74.5px;
}
.mobile_header h1 a{
	display:block;
	width: 100%; height: 100%;
}
.mobile_header .mobile_nav_btn{
	overflow: hidden;
	text-indent: -9999px;
	width: 23px; height: 20px;
	background: url(../img/common/m_nav_bar.png) center no-repeat;
	position: absolute;
	left: 21px; top: 50%;
	margin-top: -10px;
	cursor: pointer;
}
.mobile_header nav{
	width:320px; height: 100%;
	position: fixed;
	left: -140%; top: 65px;
	background-color: #fff;
	z-index: 1000;
	box-sizing:border-box;
	border-top: 2px solid #000;
	padding-bottom: 50px;
	
	-webkit-transition: all .8s ease;
	-moz-transition: all .8s ease;
	-o-transition: all .8s ease;
	transition: all .8s ease;
	
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}
.mobile_header nav .mobile_top_menu{
	
}
.mobile_header nav .mobile_top_menu:after{
	content:'';
	display: block;
	clear:both;
}
.mobile_header nav .mobile_top_menu li{
	float: left;
	width: 33%; height: 50px;
	line-height: 50px;
	text-align: center;
	box-sizing:border-box;
	font-size: 12px;
	font-weight: 400;
	position: relative;
}
.mobile_header nav .mobile_top_menu li a{
	
}
.mobile_header nav .mobile_top_menu li a:after{
	content: '';
	display: block;
	width: 1px; height: 12px;
	background-color: #ebebeb;
	position: absolute;
	top: 50%; right: 0;
	
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
}

.mobile_header nav .mobile_top_menu li:nth-of-type(3) a:after{
	right: -5%;
}

.mobile_header nav .mobile_top_menu li:last-of-type a:after{
	display: none;
}
.mobile_header nav .form_group{
	position: relative;
	width: 100%; height: 55px;
	padding: 5px 0;
	border-top: 1px solid #ddd;
	border-bottom: 1px solid #ddd;
	box-sizing:border-box;
}
.mobile_header nav .form_group input[type="text"]{
	width: 100%; height: 100%;
	font-size: 16px;
	padding-right: 43px;
	padding-left: 15px;
	box-sizing:border-box;
}
.mobile_header nav .form_group input[type="submit"]{
	width: 43px; height: 39px;
	background: transparent url(../img/common/m_search_icon.png) center no-repeat;
	border: 0;
	position: absolute;
	right: 0; top: 50%;
	margin-top: -19.5px;
	cursor: pointer;
}
.mobile_header nav .mobile_sub_menu{
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	padding: 20px 10px 30px;
	border-bottom: 1px solid #ccc;
}
.mobile_header nav .mobile_sub_menu .menu_title{
	float: left;
	width: 50%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.mobile_header nav .mobile_sub_menu .menu_title h2{
	height: 40px;
	line-height: 40px;
	text-align: left;
	font-size: 20px;
	font-weight: bold;
	color:#000;
	letter-spacing: -1px;
}
.mobile_header nav .mobile_sub_menu .menu_list{
	float: right;
	width: 50%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.mobile_header nav .mobile_sub_menu .menu_list ul{}
.mobile_header nav .mobile_sub_menu .menu_list ul li{
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #ddd;
	font-size: 16px;
	font-weight: 400;
	padding-left: 16px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.mobile_header nav .mobile_sub_menu .menu_list ul li:last-of-type{
	border-bottom: 0;
}
.mobile_header nav .mobile_sub_menu .menu_list ul li a{
	color: #333333;
}
.btn_share_naver{float:right;margin:15px!important;background:url('../img/common/share_naver.png') no-repeat; display:block;width:75px;height:18px;text-align:right;font-size:11px;}

/*개인정보팝업*/
.wrap_agree_pop{width:100%;height:100%;background:rgba(0,0,0,0.5);position:fixed;left:0;top:0;z-index:9999;}
.agree_bg{position:absolute;width:100%;height:100%;background:rgba(0,0,0,0.5);left:0;top:0;}
.agreePop{width:46%;background:#fff;position:relative;top:50%;left:50%;transform:translateY(-50%);padding:3%;margin-left:-25%;height:75%;overflow-y:auto;}
.agreePop h3{font-size:1.3em;color:#63b746;font-weight:600;}
.agreePop .closeBtnDiv{position:relative;margin-top:-4%;float:right;cursor:pointer;width:23px;height:23px;}
.agreePop .closeBtnDiv>div{width:3px;height:30px;background-color:#797979;position:absolute;left:0;right:0;}
.agreePop .closeBtnDiv>div:first-of-type{transform:rotate(45deg);}
.agreePop .closeBtnDiv>div:last-of-type{transform:rotate(-45deg);}
.agreePop p{margin:0px 12px;font-size:0.9em;margin-bottom:5px;}
.agreePop p span{margin-left:12px;display:block;}
.agreePop p.numbering{float:left;margin:0;margin-right:10px;}



/* header 영역 */
.header{width:100%;position: relative;z-index: 100;}
.login_area{height:40px;border-bottom:1px solid #efefef;font-size:0.875rem}
.header_home ul li{float: left;width:100px;border-right:1px solid #ccc;border-top:2px solid rgba(255,255,255,0);line-height:38px;text-align:center}
.header_home ul li:hover{border-top:2px solid #900040}
.header_login{float:right; margin-right:35px;}
.header_login ul li{float:left;margin:0 25px 0 8px;line-height:40px;text-align:center}
.gnb_area{position:relative;width:100%;height:100px;line-height:99px;border-bottom:1px solid #ccc;/* overflow:hidden; */z-index: 2;}
.gnb_area h1{width:16%;height: 100%;float:left;line-height: 90px;box-sizing:border-box;}
.gnb_area h1 a{display: block;width: 80%;margin-left: auto;}
.gnb_area h1 a img{}
.gnb_area .nav{width:50%;position: absolute;left: 50%;-webkit-transform: translate(-50%);-moz-transform: translate(-50%);-o-transform: translate(-50%);transform: translate(-50%);}
.gnb_area .nav ul{width:100%;font-size:16px;}
.gnb_area .nav ul li{width: 16.666%;float:left;padding-left: 3%;box-sizing: border-box}
.gnb_area .nav ul li span{position: relative;cursor: pointer;font-weight:500;font-size:17px;}
.gnb_area .nav ul li:hover span::after{content:"";display:block;width:120%;height:2px; background-color: #63b746; position: absolute; bottom: -8px; left: -10%}
.gnb_area .nav ul li:hover{color:#63b746}
/* GNB slideDown */
.gnb_dropdown{width:100%;position:absolute;top:142px;display:none;background:rgba(255,255,255,1);z-index: 1;border-bottom: 1px solid #ddd;}
.gnb_dropdown > div{width: 50%;margin: auto;}
.gnb_list{width: 100%;}
.gnb_list > li{width: 16.666%;padding-left: 3%; float: left;box-sizing: border-box}
.dropdown_list{margin:15px 0;}
.dropdown_list li{line-height:42px;font-size: 15px;letter-spacing: -1.5px;}
.dropdown_list>li a{border-bottom: 1px solid transparent;}
.dropdown_list>li a:hover{color:#63b746;border-bottom: 1px solid #63b746;}
.gnb_area div{width:16%;height: 100%;float:right;}

/* footer */
.footer{width:100%;border-top: 1px solid #efefef;padding-bottom: 60px;margin-top: 80px;}
.footer_wrap{width:1200px;height:200px;margin:60px auto 0;position: relative;}
.footer_info{float:left}
.footer_logo{height:31px;line-height:31px;margin-bottom:70px}
.footer_logo>li{float:left;}
.footer_logo>li:first-of-type{padding-left:0}
.footer_logo>li:last-of-type{border-right:0}
.footer_logo>li>h2 img{vertical-align:top}
.footer_logo>li>select{width:100%;height:31px;padding-left: 18px;/*font-size:0.875em;*/-webkit-appearance: none;-moz-appearance: none;appearance: none;background: url(../img/common/f_up_icon.png) no-repeat 98% 50%;vertical-align: top;margin-left:18px}
.footer_logo>li>select::-ms-expand{display: none}
.footer_sns>li{line-height:31px;padding-left:30px;float:left;	}
.footer_sns>li i{font-size:1.5rem;color:#575f6c;cursor: pointer;vertical-align:bottom;}
.footer_address{font-size:0.85rem}
.footer_address ul{float: left}
.footer_address ul li{line-height:25px}
.footer_company{width:525px}
.footer_company li{line-height: 40px}
.footer_banner{position: absolute;bottom: 0;right: 0;}
.footer_banner li{
	height: 46px;
    float: left;
    cursor: pointer;
    box-sizing:border-box;
    padding: 5px 15px;
}
.footer_banner li a{
	display: block;
	width: 100%; height: 100%;
}
.footer_banner li img{
	
}
.footer_link{
	float: right;
	width:43%;
}
.footer_link li{
/*     width: 80px; */
    height: 31px;
    line-height: 31px;
    float: left;
    cursor: pointer;
    text-align: center;
    font-size: 13px;
	width: auto; padding: 0 1%;
}
.footer_link li:last-of-type{
	/*width: 125px;*/
}


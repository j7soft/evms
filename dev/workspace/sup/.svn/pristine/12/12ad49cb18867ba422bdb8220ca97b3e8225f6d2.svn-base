.email_popup{
	width: 100%; height: 100%;
	position: fixed;
	left: 0; top: 0;
	z-index: 10001;

	display: none;
}
.email_popup .popup_bg{
	width: 100%; height: 100%;
	position: absolute;
	left: 0; top: 0;
	background-color: rgba(0, 0, 0, .5);
}
.email_popup .popup_body{
	background-color: #fff;
	position: absolute;
	left: 50%; top: 50%;

	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);

	box-shadow: 0 8px 16px #464646;
}
.email_popup .popup_title{
	font-size: 16px;
	color: #000;
	font-weight: bold;
	border-bottom: 1px solid #e1e1e1;
	padding: 25px;
	position: relative;
}
.email_popup .popup_title .close_btn{
	position: absolute;
	right: 15px; top: 25px;
	width: 20px; height: 20px;

	cursor: pointer;
}
.email_popup .popup_title .close_btn span{
	position: absolute;
	left: 0; top: 0;
	display: block;
	width: 100%; height: 2px;
	background-color: #2d2d2d;
}
.email_popup .popup_title .close_btn span:first-child{
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}
.email_popup .popup_title .close_btn span:last-child{
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	transform: rotate(-45deg);
}


.email_popup .popup_content{
	
}
.email_popup .popup_content .img_box{
	text-align: center;
	padding: 35px 0 15px;
}
.email_popup .popup_content .img_box img{}
.email_popup .popup_content p{
	font-size: 14px;
	color: #666666;
	padding: 0 25px 25px;
}





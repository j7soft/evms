/* 예약 - 교육 2 페이지 */
.request_area{width:625px;margin:0 auto}
.request_area h4{font-size: 2.125rem;font-weight: 500}
.request_area .edu_summary{padding:13px 0 8px;border-bottom: 3px solid #242021;font-size: 0.75rem}
.request_area .edu_date{margin-top: 80px;margin-bottom: 65px}
.request_area .edu_date li{margin-bottom: 12px;font-size: 0.875rem;font-weight: 500}
.request_area .edu_date li:first-of-type{font-size: 1.25rem}
.request_area .edu_date li:nth-of-type(2){margin-bottom: 22px}
.request_area .edu_explain{width: 100%;margin-bottom: 65px;font-size: 0.875rem;font-weight: 500}
.request_area .edu_location{width: 100%;height: 270px;padding-bottom: 115px;border-bottom: 3px solid #242021;font-size: 0.875rem;font-weight: 500}
.request_area .edu_map{width: 100%;height: 270px;float: left}
.request_area .edu_map_explain{width: 50%;height: 270px;float: left;box-sizing:border-box;padding-left: 2%;}
.request_area .edu_map_explain h3{margin-bottom: 30px;font-size: 1.225rem;font-weight: bold;}
.request_area .edu_map_explain p{font-size: 0.825rem}
.request_area .edu_request{margin-top: 20px}
.request_area .edu_request h5{margin-bottom: 50px;font-size: 1.25rem;font-weight: 700}
.request_area .edu_request li{width:100%;}
.request_area .edu_request li:after{content:'';display: block;clear:both;}
.request_area .edu_request li div{width:100%;line-height: 29px;font-size: 0.75rem;box-sizing:border-box;float: left;margin-bottom: 30px;}
.request_area .edu_request li div:after{content:'';display: block;clear:both;}
.request_area .edu_request li div.width32{width: 32%;line-height: 22px;margin-right: 2%}
.request_area .edu_request li div.width32:last-of-type{margin-right: 0}
.request_area .edu_request li div.width48{width: 48%;;margin-right: 4%;}
.request_area .edu_request li div.width48:last-of-type{margin-right: 0}
.request_area .edu_request li div.width43{width: 43%;line-height: 22px;margin-right: 3%}
.request_area .edu_request li div.width43:last-of-type{margin-right: 0}
.request_area .edu_request li div span{width: 17%;font-size: 0.875rem;font-weight: 700;float: left;}
.request_area .edu_request li div input{float: left;border-bottom: 1px solid #ccc;box-sizing:border-box;}
.request_area .edu_request li:first-of-type input{width: 89%;line-height: 29px}
.request_area .edu_request li:nth-of-type(2) input{width: 55%;line-height: 22px}
.request_area .edu_request li:nth-of-type(3) input{width: 89%;line-height: 29px}
.request_area .edu_request li:nth-of-type(4) input{width: 80%;line-height: 22px}
.request_area .edu_request li:nth-of-type(5) input{width: 80%;line-height: 36px}
.request_area .edu_request li div span:first-of-type{width: 11%;}
.request_area .edu_request li div.width32 span{width: 45%;font-size: 0.687rem;font-weight: 500;text-align: center;}
.request_area .edu_request li div.width43 span{font-size: 0.8rem;font-weight: 500;width: 18.5%;}
.request_area .edu_request li div.width48 span{width: 23%;}
.request_area .edu_request li div.width48 input{width: 77%;}
.request_area .edu_request li:last-of-type input{width: 89%;}
.request_area .edu_request li:last-of-type input[type="number"]{width: 80%;}
.request_area .edu_request li>.redstar{position:relative;font-size: 0.875rem;font-weight: 700;float: left;width: 6.5%;}
.request_area .edu_request li .redstar::before{content:"*";display:block;color: #ec1c24; position: absolute; left: -11px; top: 0;}
.request_area .edu_request li input, .request_area .edu_request li label{}
.request_area .edu_request li input.edu_sum{width:15px;margin:0 8px;position: relative;top: 12px}
.request_area .edu_request li input.edu_number{width: 200px;line-height: 22px;top: -5px;text-align: center}
.request_area .edu_agree{margin-top: 40px}
.request_area .edu_agree ul li{line-height: 36px;font-size: 0.75rem}
.request_area .edu_agree button{width:100%;height: 36px;margin-top: 30px;background: #a6a8ab;font-weight: 700;color: #fff}
.request_area .edu_agree input{margin-right:8px;position: relative;top: 3px}
.request_area .edu_agree ul li a{margin-left: 10px;font-weight: 700}



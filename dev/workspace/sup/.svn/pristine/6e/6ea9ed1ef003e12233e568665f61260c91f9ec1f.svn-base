.content_message{width:1200px;margin:0 auto}
.content_message h4{width: 522px;height: 50px;margin:0 auto;background: url('../img/introduction/message_h4.png') no-repeat}
.content_message h4 p{width: 256px;line-height: 50px;display: inline-block;color: #fff;text-align: center}
.content_message .tab_box2{display: none}
.message_wrap{margin-top: 80px}
.ceo_img{width: 30%;float: left;text-align: center}
.message_box{width: 100%;float: none;}
.message_box h5{font-size: 1.625rem;font-weight: 500;border-bottom: 2px solid #ddd;padding-bottom: 24px;text-indent: 15px;}
.message_box h6{font-size: 1.125em;font-weight: bold;margin-top: 30px;margin-bottom: 30px;}
.message_box ul{width:80%;margin-bottom: 60px;}
.message_box ul li{line-height: 1.5;margin-bottom: 30px;}
.message_box ul li p{font-size: 0.875rem;color: #2d2d2d;}
.message_box p{font-weight: 500}
.message_box p {position: relative;}
.message_box ul li p:first-of-type{margin-bottom: 10px;}
.message_box ul li p strong{font-size: 0.905rem;}
.message_box p span{position: absolute; left: 3px; top: 0;}
.message_box p.text_p,
.message_box p.num_p{
	padding-left: 25px;
}
.message_box p.word_p{
	padding-left: 35px;
}
.message_box p.word_p .word{
	left: 10px;
}


.message_box table{
	border: 2px solid #000;
	border-collapse: collapse;
}
.message_box table tr{}
.message_box table tr th,
.message_box table tr td{
	text-align: center;
	border: 1px solid #000;
	padding: 10px 0;
}
.message_box table tr th{
	background-color: #ccc;
}

/* 새활용이란 */
.introduction{width:100%;}
.introduction div, .introduction p{text-align: center;}
.introduction p{font-size:1.813rem; font-weight: bold; color:#231f20;}
.introduction p:nth-child(2), .introduction p:nth-child(4){margin-top:20px; margin-bottom:22px;}
.introduction .icon01 img:nth-child(1){position:relative; left:-130px; top:-50px;}
.introduction .icon01 img:nth-child(2){position:relative; left:130px; top:-50px;}
.introduction .text01{position:relative; top:-60px;font-size:22px; font-weight: normal; color:#8f1843;}
.introduction .textBox{padding-left:7.5%;}
.introduction .textBox>p{line-height:2; font-size:1rem; font-weight:normal; color:#58595b; text-align: left;}
#section .up_img{margin-top:150px;}
#section .up_img>img{width:100%;}

/* 사업 소개 */
.business .Box01{width:100%;}
.business .Box01 .leftBox{width:23.333%; float:left;}
.business .Box01 .leftBox>img{margin-bottom:115px;}
.business .Box01 .leftBox p{line-height: 2; font-size:0.9rem; color:#58595b;}
.business .Box01 .mobile_box{margin: 30px 60px; padding-top: 30px;border-top: 1px solid #ddd;}
.business .Box01 .mobile_box p{line-height: 2; font-size:0.9rem; color:#58595b;}
.business .Box01 .mobile_box p span{padding-right:8px;font-size: 0.9rem; color:#000;}
.business .Box01 .leftBox p span{padding-right:8px;font-size: 0.9rem; color:#000;}
.business .Box01 .rightBox{width:76%; margin-top:30px; margin-bottom:100px; position:relative; float:right;}
.business .Box01 .rightBox h4{padding-left:60px; line-height:1.5; font-size:1.813rem;}
.business .Box01 .rightBox > p{margin-top:30px; padding-left:60px; line-height: 2; font-size:1rem;}
.business .Box01 .rightBox .iconBox{position:absolute; bottom:0; float:left;}
.business .Box01 .rightBox .iconBox div img{cursor: pointer;}
.business .Box01 .rightBox .imgBox{margin-top:110px;float:right;}
.business .Box01 .rightBox .imgBox img{width: 100%;}
.business .Box02{padding:50px 0 70px 0;border-top:1px solid #d3d5d6; border-bottom:1px solid #d3d5d6; clear:both;}
.business .Box02 h4{margin-bottom:25px;font-size:1.813rem; text-align: center;font-weight: 400;}
.business .Box02>p:nth-child(2){
    width: 800px;
    margin: auto;
    line-height: 1.8;
    font-size: 0.985rem;
    color: #58595b;
    text-align: center;
   	letter-spacing: -1px;
}
.business .Box02 .textBox{width:100%; margin-top:80px;}
.business .Box02 .textBox .leftBox{margin-top:10px; float:left;}
.business .Box02 .textBox .leftBox p{line-height: 2; color:#58595b;}
.business .Box02 .textBox .leftBox p span{width:190px; margin-right:20px; display: inline-block; font-size:1rem; color:#000;letter-spacing: -0.5px;}
.business .Box02 .textBox .leftBox .textBox2{width:500px; height:160px; margin-top:34px; padding:30px; border:1px solid #e8e9e9; font-size:0.8rem; color:#808184; box-sizing:border-box;}
.business .Box02 .textBox .rightBox{float:right;}
.business .Box03{margin-top:100px;}
.business .Box03 h4{margin-bottom:25px; font-size:1.813rem; text-align: center;font-weight: 400;letter-spacing: -1px;}
.business .Box03>p:nth-child(2){
    width: 800px;
    margin: auto;
    line-height: 1.8;
    font-size: 0.985rem;
    color: #58595b;
    text-align: center;
    letter-spacing: -1px;
}
.business .Box03 .textBox{width:100%; margin-top:40px;}
.business .Box03 .textBox .leftBox{margin-top:120px; margin-left:82px; float:left;}
.business .Box03 .textBox .leftBox>div:nth-child(1){ height: 100px; border:1px solid #e8e9e9; padding:13px; text-align: center; box-sizing:border-box;}
.business .Box03 .textBox .leftBox>div:nth-child(1) p{line-height: 1.5;}
.business .Box03 .textBox .leftBox>div:nth-child(2){margin:30px 0;text-align: center;}
.business .Box03 .textBox .leftBox>p:nth-last-child(1){line-height: 1.5; position:relative; font-size:1.400rem;color:#58595b; text-align: center;font-weight: bold;}
.business .Box03 .textBox .leftBox>p:nth-last-child(1) span{border-bottom:1px solid #939090;}
.business .Box03 .textBox .rightBox{float:right;}


/* SUP소개 조직도 시작 */
.Organization{}
.Organization  > .img_box{
	margin-bottom: 190px;
	text-align: center;
}

.Organization .section{
	margin-bottom: 50px;
}

.Organization .section:last-child{
	margin-bottom: 0;
}

.Organization .section .team_title{
	font-size: 22px;
	font-weight: bold;
	color: #333333;
	margin-bottom: 38px;
	margin-left: 43px;
	position: relative;
}
.Organization .section .team_title:after{
	content: '';
	display: block;
	width: 14px; height: 14px;
	background-color: #333333;
	position: absolute;
	left: -30px; top: 50%;
	margin-top: -6px;
}
.Organization .section .team{
	padding-top: 50px;
	padding-bottom: 50px;
	border-top: 3px solid #ebebeb;
	border-bottom: 3px solid #ebebeb;
}

.Organization .section .team .info_box{
	text-align: center;
}

.Organization .section .team .info_box:nth-child(n+5){
	margin-top: 50px;
}

.Organization .section .team .info_box .img_box{
	text-align: center;
	margin-bottom: 19px;
}

.Organization .section .team .info_box .name{
	font-size: 18px;
	color: #666666;
	font-weight: bold;
	margin-bottom: 5px;
}
.Organization .section .team .info_box ul{
	margin-bottom: 17px;
}
.Organization .section .team .info_box ul li{
	width: 85%; margin: auto;
	font-size: 11px;
	color: #858585;
}

.Organization .section .team .info_box ul li span{
	position: relative;
}


/* 오시는길 */
.Location{}
.Location #map_canvas{
	width: 893px; height: 520px;
	border: 3px solid #c8c8c8;
	margin: auto;
	margin-bottom: 75px;

	box-sizing: border-box;
}
.Location .text_box ul li:after{
	content: '';
	display: block;
	clear: both;
}
.Location .text_box{
	width: 898px; margin: auto;
}
.Location .text_box ul{}
.Location .text_box ul li{
	margin-bottom: 30px;
}
.Location .text_box ul li span{
	display: block;
}
.Location .text_box ul li span.left{
	float: left;
	width: 14%;
	font-size: 1.1rem;
	font-weight: bold;
}
.Location .text_box ul li span.right{
	float: right;
	width: 86%;
	color: #797979;
	font-size: 0.9rem;
	margin-bottom: 10px;

	padding-left: 3.5%;
	box-sizing: border-box;
}

.Location .text_box ul li span.right span{
	text-indent: 15px;
	margin-bottom: 8px;
	position: relative;
}
.Location .text_box ul li span.right span:last-of-type{
	margin-bottom: 0;
}
.Location .text_box ul li span.right span:after{
	content: '';
	display: block;
	width: 10px; height: 10px;
	background-color: #000;
	position: absolute;
	left: 0; top: 50%;
	margin-top: -5px;
	border-radius: 50%;
}
.Location .text_box ul li span.right span:nth-of-type(1):after{background-color: #036cd9;}
.Location .text_box ul li span.right span:nth-of-type(2):after{background-color: #76c617;}
.Location .text_box ul li span.right span:nth-of-type(3):after{background-color: #eb3124;}
.Location .text_box ul li span.right span:nth-of-type(4):after{background-color: #f1bc32;}

.Location .text_box ul li span.icon1{
	background: url(../img/introduction/lo_icon1.png) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
}

.Location .text_box ul li span.icon2{
	background: url(../img/introduction/lo_icon2.png) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
}

.Location .text_box ul li span.icon3{
	background: url(../img/introduction/lo_icon3.png) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
}
.Location .text_box ul li:last-of-type span.icon2{
	height: 26px;
}
.mobile_img{
	display: none;
}
.mobile_box{
	display: none;
}
.mobile_hidden{
	display: none;
}

@media all and (max-width:1201px) {
	#section .intro{
		width: 100%;
	}
	
	.introduction .textBox {
		width: 85%; margin: auto;
	    padding-left: 0;
	}
	.introduction .textBox>p{
		line-height: 1.5;
	}
	
	#section .intro {
	    width: 100%;
	    /*margin: 90px auto 0;*/
	}
	
	.business .Box01 .rightBox .iconBox{
		display: none; 
	}
	.content_message{
		width: 100%;
	}
	.message_box ul{
		width: 100%;
	}
	
	.Organization > .img_box img{
		width: 100%;
	}
	
	
}

@media all and (max-width:1171px) {
	.business .Box01 .leftBox{
		display: none;
	}
	.business .Box01 .rightBox{
		float: none;
		width: 100%;
	}
	.mobile_box{
		display: block;
	}
	.business .Box01 .rightBox .imgBox{
		width: 80%; margin: 80px auto 0;
		float: none;
	}
	.business .Box01 .rightBox h4{
		width: 85%;
		margin: auto;
		padding-left: 0;
	}
	.business .Box01 .rightBox > p {
		width: 85%;
		margin: auto;
	    margin-top: 20px;
	    padding-left: 0;
	    line-height: 1.5;
	    font-size: 0.9rem;
	}
	img.mobile_hidden{
		display: inline-block;
		margin-top: 10px;
	}
	.business .Box02 .textBox .leftBox{
		float: none;
		margin-bottom: 30px;
	}
	.business .Box02 .textBox{
		width: 600px; margin: 80px auto 40px;
	}
	.business .Box02 .textBox .leftBox .textBox2{
		width: 600px;
		margin: auto;
		margin-top: 20px;
	}
	.business .Box02 .textBox .rightBox{
		float: none;
	}
	.business .Box02 .textBox .rightBox div{
		text-align: center;
	}
	.business .Box02 .textBox .rightBox img{
		width: 90%;
	}
	.business .Box03 .textBox .leftBox{
		float: none;
		margin: 60px auto 30px;
	}
	.business .Box03 .textBox .rightBox{
		float: none;
	}
	.business .Box03 .textBox .rightBox div{
		width: 80%; margin: auto;
	}
	.business .Box03 .textBox .rightBox img{
		width: 100%;
	}
	.business .Box03>p:nth-child(2){
		width: 80%;
	}
	.business .Box02>p:nth-child(2){
		width: 80%;
	}
}


@media all and (max-width:960px) {
	.message_box{
		width: 90%;
		margin: auto;
	}
}


@media all and (max-width:600px) {
	.business .Box02 .textBox{
		width: 100%;
	}
	.business .Box02 .textBox .leftBox p{
		line-height: 1.5;
		width: 80%;
		margin: auto;
		margin-bottom: 20px;
	}
	.business .Box02 .textBox .leftBox p span{
		width: 100%;
		display: block;
	}
	.business .Box02 .textBox .leftBox .textBox2{
		width: 90%;
		padding: 15px;
		height: auto;
	}
	.business .Box02 .textBox .leftBox p:last-of-type{
		margin-bottom: 0;
	}
}

@media all and (max-width:480px) {
	.introduction .textBox{
		    margin: 0 auto 50px;
	}
	.up_img{
		display: none;
	}
	.mobile_img{
		display: block;
	}
	.mobile_img img{
		width: 100%;
	}
	.business .Box01 .rightBox > p{
		width: 90%;
		font-size: 0.8rem;
	}
	.business .Box01 .rightBox h4{
	    font-size: 1.513rem;
	}
	.business .Box01 .mobile_box{
		width: 90%;
		margin: 30px auto;
	}
	.route{
	    top: -15px;
	}
	.business .Box01 .rightBox .imgBox{
		width: 90%;
	}
	.business .Box03 h4{
	    font-size: 1.513rem;
	}
	.business .Box03>p:nth-child(2){
		font-size: 0.885rem;
	}
	.business .Box03 .textBox .rightBox div{
		width: 90%;
	}
	.business .Box03 .textBox .leftBox>p:nth-last-child(1){
		font-size: 1.3rem;
	}
	
	
	
}





/* 오시는길 */
.Location{}
#map{
	width: 50%; height:300px;
	margin: auto;
	margin-bottom: 50px;
	text-align: center;
}
.content_h3bx .location{
	background-image: url(/img/introduction/location.png);
	height: 400px;
   	width: 700px;
  	border: 1px solid gray;
   	margin: 0 auto;
   	margin-bottom: 50px;
	box-sizing: border-box;
}
.Location .text_box ul li:after{
	content: '';
	display: block;
	clear: both;
}
.Location .text_box{
	width: 898px; margin: auto;
}
.Location .text_box ul{}
.Location .text_box ul li{
	margin-bottom: 30px;
}
.Location .text_box ul li span{
	display: block;
}
.Location .text_box ul li span.red_strong{
	font-weight: bold;
	color: red;
}
.Location .text_box ul li span.left{
	float: left;
	width: 14%;
	font-size: 1.1rem;
	font-weight: bold;
}
.Location .text_box ul li span.right{
	float: right;
	width: 86%;
	color: #797979;
	font-size: 0.9rem;
	margin-bottom: 10px;

	padding-left: 3.5%;
	box-sizing: border-box;
}

.Location .text_box ul li span.right span{
	text-indent: 12px;
	margin-bottom: 8px;
	position: relative;
}
.Location .text_box ul li span.right span:last-of-type{
	margin-bottom: 0;
}
.Location .text_box ul li span.right span:after{
	content: '';
	display: block;
	width: 10px; height: 10px;
	background-color: #000;
	position: absolute;
	left: 0; top: 50%;
	margin-top: -5px;
	border-radius: 50%;
}
.Location .text_box ul li span.right span:nth-of-type(1):after{background-color: #036cd9;}
.Location .text_box ul li span.right span:nth-of-type(2):after{background-color: #76c617;}
.Location .text_box ul li span.right span:nth-of-type(3):after{background-color: #eb3124;}
.Location .text_box ul li span.right span:nth-of-type(4):after{background-color: #f1bc32;}

.Location .text_box ul li span.icon1{
	background: url(../img/introduction/img_subway.svg) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
	background-size:20px 20px;
}

.Location .text_box ul li span.icon2{
	background: url(../img/introduction/img_bus.svg) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
	background-size:20px 20px;
}

.Location .text_box ul li span.icon3{
	background: url(../img/introduction/img_car.svg) 0 0 no-repeat;
	line-height: 1.5;
	font-weight: bold;
	margin-bottom: 3px;
	background-size:20px 20px;
}
.Location .text_box ul li:last-of-type span.icon2{
	height: 26px;
}

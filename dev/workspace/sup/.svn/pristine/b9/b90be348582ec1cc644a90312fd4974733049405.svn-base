/* 사업 소개 */
.textBox_2{padding:50px 0;padding-left:55px;}
.textBox_2 p{margin-bottom:15px;overflow:hidden;}
.textBox_2 span{display:block;float:left;padding:5px 0px;border-radius:10px;border:2px solid #63b647; color:#63b647;width:30%;box-sizing:border-box;text-align:center}
.textBox_2 em{display:block;float:right;width:70%;box-sizing:border-box;padding-left:20px;line-height:35px;}
.business .Box01{width:95%;max-width:1200px;margin:0 auto;overflow:hidden;position:relative;}
.business .Box01 .leftBox{width:23.333%; float:left;}
.business .Box01 .leftBox>div{position:absolute;bottom:100px}
.business .Box01 .leftBox p{line-height: 2; font-size:0.9rem; color:#333;}
.business .Box01 .mobile_box{margin: 30px 60px; padding-top: 30px;border-top: 1px solid #ddd;}
.business .Box01 .mobile_box p{line-height: 2; font-size:0.9rem; color:#666;}
.business .Box01 .mobile_box p span{padding-right:8px;font-size: 0.9rem; color:#000;}
.business .Box01 .leftBox p span, .business .Box01 .leftBox p strong{padding-right:8px;font-size: 0.9rem; color:#000;}
.business .Box01 .rightBox{width:76%; margin-top:30px;  float:right;}
.business .Box01 .rightBox h4{padding-left:55px; line-height:1.5; font-size:35px;font-weight:bold;}
.business .Box01 .rightBox > p{margin-top:15px; padding-left:55px; line-height: 1.5; font-size:16px;color:#666;}
.business .Box01 .rightBox .iconBox{position:absolute; top:10px; right:10px;overflow:hidden;}
.business .Box01 .rightBox .iconBox>div{float:left;}
.business .Box01 .rightBox .iconBox div img{cursor: pointer;}
.business .Box01 .rightBox .imgBox{margin-top:20px;float:right;}
.business .Box01 .rightBox .imgBox img{width: 100%;}
.business .Box02{padding-top:50px;border-top:1px solid #d3d5d6; clear:both;max-width:1200px;margin:0 auto;width:95%;}
.business .Box02 h4{margin-bottom:25px;font-size:35px; text-align: left;font-weight: bold;}
.business .Box02>p:nth-child(2){
    width: 800px;
    margin: auto;
    line-height: 1.8;
    font-size: 0.985rem;
    color: #58595b;
    text-align: center;
   	letter-spacing: -1px;
}
.business .Box02 .textBox{width:100%; margin-top:80px;}
.business .Box02 .textBox .leftBox{margin-top:10px; float:left;}
.business .Box02 .textBox .leftBox p{line-height: 2; color:#58595b;}
.business .Box02 .textBox .leftBox p span{width:190px; margin-right:20px; display: inline-block; font-size:1rem; color:#000;letter-spacing: -0.5px;}
.textBox2{padding:20px 0px 0px;}
.textBox2 span{padding:0px; border:none;width:auto;font-size:20px;font-weight:bold;margin-right:15px;}
.business .Box02 .textBox .leftBox .textBox2{width:500px; height:160px; margin-top:34px; padding:30px; border:1px solid #e8e9e9; font-size:0.9rem; color:#808184; box-sizing:border-box;}
.business .Box02 .textBox .rightBox{float:right;}
.business .Box03{margin-top:100px;}
.business .Box03 h4{margin-bottom:25px; font-size:1.813rem; text-align: center;font-weight: 400;letter-spacing: -1px;}
.business .Box03>p:nth-child(2){
    width: 800px;
    margin: auto;
    line-height: 1.8;
    font-size: 0.985rem;
    color: #58595b;
    text-align: center;
    letter-spacing: -1px;
}
.business .Box03 .textBox{width:100%; margin-top:40px;}
.business .Box03 .textBox .leftBox{margin-top:120px; margin-left:82px; float:left;}
.business .Box03 .textBox .leftBox>div:nth-child(1){ height: 100px; border:1px solid #e8e9e9; padding:13px; text-align: center; box-sizing:border-box;}
.business .Box03 .textBox .leftBox>div:nth-child(1) p{line-height: 1.5;}
.business .Box03 .textBox .leftBox>div:nth-child(2){margin:30px 0;text-align: center;}
.business .Box03 .textBox .leftBox>p:nth-last-child(1){line-height: 1.5; position:relative; font-size:1.400rem;color:#58595b; text-align: center;font-weight: bold;}
.business .Box03 .textBox .leftBox>p:nth-last-child(1) span{border-bottom:1px solid #939090;}
.business .Box03 .textBox .rightBox{float:right;}

/* 새활용 거리 */
.street_area{
  width: 1200px;
  margin: 0 auto;
}
.box_street{
  height: 260px;
  margin-bottom: 60px;
}
.box_street *{
  box-sizing: border-box;
}
.box_street .box_left{
  float: left;
  position: relative;
  width: 50%; height: 100%;
  padding: 20px 55px 30px 20px;
  border-bottom: 1px solid #d7d7d7;
}
.box_street h4{
  margin-bottom: 10px;
  font-size: 24px; line-height: 36px; color: #8f1843;
}
.box_street p{
  line-height: 28px; color: #6d6e70;
  word-break: keep-all;
}
.box_street .btn_more{
  position: absolute; right: 60px; bottom: 30px;
  font-size: 12px; color: #6d6e70;
}
.box_street .btn_more p{display:inline-block;}
.box_street .btn_more:after{
  content: ">";
  display:inline-block;
  margin-left:5px;
}
.box_street .box_right{
  float: right;
  width: 50%; height: 100%;
}
.box_street .box_right img{
  float: left;
  width: 100%; height: 100%;
}
.box_street .box_img2n img{
  width: 49%;
}
.box_street .box_img2n img:nth-of-type(2){
  float: right;
}
.list_street{
  padding: 0 37px 60px;
}
.list_street li{
  float: left;
  width: 375px;
  text-align: center;
}
.list_street h5{
  margin: 35px 0;
  font-size: 30px; font-weight: bold; color: #333;
}

